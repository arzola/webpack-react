{
    "extends": "airbnb",
    "rules": {
        "semi": [2,"never"],
        "react/prefer-stateless-function": [
            0
        ],
        "react/jsx-filename-extension": [1,{"extensions":[".js",".jsx"]}],
        "import/no-extraneous-dependencies": [
            "error",
            {"devDependencies": true}
        ]
    }
}